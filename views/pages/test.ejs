<h1>Alumnos</h1>
<table id="alumno" class="table striped" style="width: 50%;margin: auto">
  <thead>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>DNI</th>
    <th>Edad</th>
    <th>Ir a objeto en DB</th>
  </tr>
  </thead>
  <tbody>
  <% for(var i = 0;i < a.length;i++){ %>
  <tr>
    <td><%= a[i].nombre %></td>
    <td><%= a[i].apellido %></td>
    <td><%= a[i].DNI %></td>
    <td><%= a[i].edad %></td>
    <td>
      <a href="/Alumno/<%= a[i].id %>" class="btn-info btn">
        <i class="fas fa-eye"></i>
      </a>
    </td>
  </tr>
  <% } %>
  </tbody>
</table>
